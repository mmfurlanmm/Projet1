
@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <title>Merci d'avoir voté</title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="../../Content/style.css">

</head>


<body>
    <header>
        <img id="wolf" alt="Loup" src="../../Content/wolf.jpg" />
        <p id=APLogo>AlphaPoll</p>

    </header>

 

    <h1>Votre vote a bien été pris en compte </h1>
